    "@mdx-js/loader": "^1.5.1",
    "@next/mdx": "^9.1.3",
    "@svgr/cli": "^4.3.3",
    "@types/node": "^12.12.8",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "2.8.0",
