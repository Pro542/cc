{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "alias": "chibicode.com",
  "name": "chibicode.com",
  "github": {
    "silent": true
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "status": 404, "dest": "404.html" }
  ]
}
